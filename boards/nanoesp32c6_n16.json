{
    "build": {
      "arduino":{
        "ldscript": "esp32c6_out.ld",
        "partitions": "default_16MB.csv",
        "memory_type": "qio_qspi"
      },
      "core": "esp32",
      "extra_flags": [
        "-DARDUINO_ESP32C6_DEV",
        "-DARDUINO_USB_MODE=1",
        "-DARDUINO_USB_CDC_ON_BOOT=1"
      ],
      "f_cpu": "160000000L",
      "f_flash": "40000000L",
      "flash_mode": "qio",
      "psram_type": "qspi",
      "hwids": [
        [
          "0x1A86",
          "0x55D3"
        ]
      ],
      "mcu": "esp32c6",
      "variant": "esp32c6"
    },
    "connectivity": [
      "wifi",
      "bluetooth"
    ],
    "debug": {
      "default_tool": "esp-builtin",
      "onboard_tools": [
        "esp-builtin"
      ],
      "openocd_target": "esp32c6.cfg"
    },
    "frameworks": [
      "arduino",
      "espidf"
    ],
    "name": "NanoESP32-C6-N16 (16MB QD)",
    "upload": {
      "flash_size": "16MB",
      "maximum_ram_size": 327680,
      "maximum_size": 16777216,
      "require_upload_port": true,
      "speed": 1152000
    },
    "url": "https://github.com/wuxx/nanoESP32-C6",
    "vendor": "MuseLab"
  }
